    <urn:Name>ContinuersEligibleToEnrol_Letter</urn:Name>
    <urn:Description>Chase letter sent to Continuers</urn:Description>
    <urn:Owner>brmuser</urn:Owner>
    <urn:ListType>student</urn:ListType>
    <urn:ExecutionContext>
      <urn:ExpressionName>ContinuersEligibleToEnrol_Letter</urn:ExpressionName>
    </urn:ExecutionContext>
  </urn:Population>
  <urn:Expression xmlns:urn="urn:sungardhe:relate:importhandlers:selection:1.1">
    <urn:Name>ContinuersEligibleToEnrol_Letter</urn:Name>
    <urn:Description>Selects Continuing students who are eligible to enrol</urn:Description>
    <urn:Application>Relate</urn:Application>
    <urn:Creator>brmuser</urn:Creator>
    <urn:RawSql>true</urn:RawSql>
    <urn:ListType>student</urn:ListType>
    <urn:ExpressionDefinition>
      <urn:XmlExpression>&lt;?xml version="1.0" encoding="UTF-8" standalone="yes"?&gt;&lt;Expression xmlns="urn:com.sungardhe.common.services.selection.querygenerator"&gt;&lt;DatabaseColumnName&gt;SPRIDEN_PIDM&lt;/DatabaseColumnName&gt;&lt;DatabaseLocationName&gt;Student&lt;/DatabaseLocationName&gt;&lt;DataType&gt;number&lt;/DataType&gt;&lt;Subquery&gt;&lt;Statement&gt;select  SPRIDEN_PIDM FROM SPRIDEN, SGBSTDN, SFBETRM, SPBPERS   where  SGBSTDN.SGBSTDN_PIDM = SFBETRM.SFBETRM_PIDM and   SGBSTDN.SGBSTDN_TERM_CODE_EFF = SFBETRM.SFBETRM_TERM_CODE and     SGBSTDN.SGBSTDN_PIDM = SPRIDEN.SPRIDEN_PIDM and      SPRIDEN.SPRIDEN_CHANGE_IND IS NULL and     SFBETRM.SFBETRM_ESTS_CODE in ('EL','EE','PE','RE','YE') and     SGBSTDN.SGBSTDN_STST_CODE = 'AS' and   NOT EXISTS (select  'X' from gtvsdax               	   where  gtvsdax_internal_code = 'PRSESS_CRS' and   	   gtvsdax_internal_code_group = 'PRSESS_CRS_CODE' and  	   gtvsdax_external_code = SGBSTDN_PROGRAM_1) and   NOT EXISTS (   (select  'X' from gtvsdax                        where  gtvsdax_internal_code = 'NURSING' and                      gtvsdax_internal_code_group = 'BRM' and      		gtvsdax_external_code = SGBSTDN_PROGRAM_1 and     		baninst1.FN$GET_FINAL_YR_IND(SGBSTDN.SGBSTDN_PIDM,SGBSTDN.SGBSTDN_TERM_CODE_EFF,null) = 'Y' )             ) and  SGBSTDN.SGBSTDN_CAMP_CODE not in ('DL','EL','SI','VT') and  SPBPERS_PIDM = SGBSTDN.SGBSTDN_PIDM and     NVL(SPBPERS_DEAD_IND,'N') = 'N' and    SGBSTDN.SGBSTDN_TERM_CODE_ADMIT &amp;lt; SGBSTDN.SGBSTDN_TERM_CODE_EFF and   SGBSTDN.SGBSTDN_TERM_CODE_EFF =                 (select STVTERM_CODE from STVTERM                 where trunc(SYSDATE) &amp;gt;= trunc(STVTERM_START_DATE) and                 trunc(SYSDATE)   &amp;lt;= trunc(STVTERM_END_DATE)) and  exists  (select distinct 'Y' from baninst1.BRM_COMMUNICATIONS_CAMPAIGN bcc   where bcc.BANNER_PIDM  = SGBSTDN_PIDM  and   bcc.BRM_CAMPAIGN_NAME like 'Enrolment_Chase_Continuers_'||SGBSTDN.SGBSTDN_TERM_CODE_EFF||'%' and   bcc.BRM_TEMPLATE_NAME like 'Enrolment_Chase_Continuers_'||SGBSTDN.SGBSTDN_TERM_CODE_EFF||'% - Chase_email3'  and   bcc.BRM_COMMUNICATION_RECORD = 'EMAIL' and   TO_DATE(TO_CHAR(bcc.BRM_DATE_SENT_COMMUNICATION, 'YYYY-MON-DD HH24:MI:SS'),'YYYY-MON-DD HH24:MI:SS') &amp;gt;=  		(select STVTERM_START_DATE from STVTERM where  STVTERM_CODE = SGBSTDN.SGBSTDN_TERM_CODE_EFF)  )&lt;/Statement&gt;&lt;Bindings&gt;&lt;Timestamp&gt;0&lt;/Timestamp&gt;&lt;Size&gt;0&lt;/Size&gt;&lt;/Bindings&gt;&lt;/Subquery&gt;&lt;/Expression&gt;</urn:XmlExpression>
    </urn:ExpressionDefinition>
  </urn:Expression>
