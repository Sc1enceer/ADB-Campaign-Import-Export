    <urn:CommunicationMethods>
      <urn:LetterTemplate>
        <urn:PaperSettings>
          <urn:Dimension>in</urn:Dimension>
          <urn:TopMargin>1.0</urn:TopMargin>
          <urn:BottomMargin>0.2</urn:BottomMargin>
          <urn:LeftMargin>1.0</urn:LeftMargin>
          <urn:RightMargin>1.0</urn:RightMargin>
