  <urn:Rule xmlns:urn="urn:sungardhe:relate:importhandlers:rules:1.0">
    <urn:Name>Check_PM_exists</urn:Name>
    <urn:Description>Checks if there is a current PM address</urn:Description>
    <urn:RuleStatement>
      <urn:Type>SQL_PREPARED_STATEMENT</urn:Type>
      <urn:Content>SELECT DECODE(count(*),0,'N','Y') AS o_pm_rec
FROM   spraddr sp1       
WHERE sp1.spraddr_pidm = ? AND
sp1.spraddr_atyp_code = 'PM' AND 
(trunc(sp1.spraddr_to_date) &gt;= trunc(sysdate) or sp1.spraddr_to_date is null) AND
NVL(sp1.spraddr_status_ind,'X') &lt;&gt; 'I'</urn:Content>
    </urn:RuleStatement>
    <urn:RuleArgument>
      <urn:Name>o_pm_rec</urn:Name>
      <urn:Description>Does PM record exist (Y/N)</urn:Description>
      <urn:Type>java.lang.Character</urn:Type>
      <urn:OutputFlag>true</urn:OutputFlag>
      <urn:OutputOrder>1</urn:OutputOrder>
      <urn:OutputGuaranteed>true</urn:OutputGuaranteed>
    </urn:RuleArgument>
    <urn:RuleArgument>
      <urn:Name>_pidm</urn:Name>
      <urn:Description>Pidm</urn:Description>
      <urn:Type>java.lang.String</urn:Type>
      <urn:InputFlag>true</urn:InputFlag>
      <urn:InputOrder>1</urn:InputOrder>
      <urn:InputRequired>true</urn:InputRequired>
    </urn:RuleArgument>
    <urn:SystemReqInd>true</urn:SystemReqInd>
  </urn:Rule>
  <urn:Rule xmlns:urn="urn:sungardhe:relate:importhandlers:rules:1.0">
    <urn:Name>Get_Todays_Date</urn:Name>
    <urn:Description>Get the current date</urn:Description>
    <urn:RuleStatement>
      <urn:Type>SQL_PREPARED_STATEMENT</urn:Type>
      <urn:Content>select to_char(sysdate,'ddth Month yyyy') as v_todays_date from dual</urn:Content>
    </urn:RuleStatement>
    <urn:RuleArgument>
      <urn:Name>v_todays_date</urn:Name>
      <urn:Description>Todays Date</urn:Description>
      <urn:Type>java.lang.String</urn:Type>
      <urn:OutputFlag>true</urn:OutputFlag>
      <urn:OutputOrder>1</urn:OutputOrder>
      <urn:OutputGuaranteed>true</urn:OutputGuaranteed>
    </urn:RuleArgument>
    <urn:SystemReqInd>true</urn:SystemReqInd>
  </urn:Rule>
  <urn:Rule xmlns:urn="urn:sungardhe:relate:importhandlers:rules:1.0">
    <urn:Name>Check_TT_exists</urn:Name>
    <urn:Description>Checks if there is a current TT address</urn:Description>
    <urn:RuleStatement>
      <urn:Type>SQL_PREPARED_STATEMENT</urn:Type>
      <urn:Content>SELECT DECODE(count(*),0,'N','Y') AS o_tt_rec
FROM   spraddr sp1       
WHERE sp1.spraddr_pidm = ? AND
sp1.spraddr_atyp_code = 'TT' AND 
(trunc(sp1.spraddr_to_date) &gt;= trunc(sysdate) or sp1.spraddr_to_date is null) AND
NVL(sp1.spraddr_status_ind,'X') &lt;&gt; 'I'</urn:Content>
    </urn:RuleStatement>
    <urn:RuleArgument>
      <urn:Name>o_tt_rec</urn:Name>
      <urn:Description>Does TT record exist</urn:Description>
      <urn:Type>java.lang.Character</urn:Type>
      <urn:OutputFlag>true</urn:OutputFlag>
      <urn:OutputOrder>1</urn:OutputOrder>
      <urn:OutputGuaranteed>true</urn:OutputGuaranteed>
    </urn:RuleArgument>
    <urn:RuleArgument>
      <urn:Name>_pidm</urn:Name>
      <urn:Description>Pidm</urn:Description>
      <urn:Type>java.lang.String</urn:Type>
      <urn:InputFlag>true</urn:InputFlag>
      <urn:InputOrder>1</urn:InputOrder>
      <urn:InputRequired>true</urn:InputRequired>
    </urn:RuleArgument>
    <urn:SystemReqInd>true</urn:SystemReqInd>
  </urn:Rule>
  <urn:Rule xmlns:urn="urn:sungardhe:relate:importhandlers:rules:1.0">
    <urn:Name>Get_Term_Code</urn:Name>
    <urn:Description>Determine current Term Code</urn:Description>
    <urn:RuleStatement>
      <urn:Type>SQL_PREPARED_STATEMENT</urn:Type>
      <urn:Content>SELECT stvterm_code AS o_current_term
  FROM STVTERM
  WHERE TRUNC (SYSDATE) &gt;= TRUNC (stvterm_start_date)
  AND TRUNC (SYSDATE)   &lt;= TRUNC (stvterm_end_date)</urn:Content>
    </urn:RuleStatement>
    <urn:RuleArgument>
      <urn:Name>o_current_term</urn:Name>
      <urn:Description>Current Term Code</urn:Description>
      <urn:Type>java.lang.String</urn:Type>
      <urn:OutputFlag>true</urn:OutputFlag>
      <urn:OutputOrder>1</urn:OutputOrder>
      <urn:OutputGuaranteed>true</urn:OutputGuaranteed>
    </urn:RuleArgument>
    <urn:SystemReqInd>true</urn:SystemReqInd>
  </urn:Rule>
  <urn:Rule xmlns:urn="urn:sungardhe:relate:importhandlers:rules:1.0">
    <urn:Name>Get_Date_for_Continuers_Letter_V2</urn:Name>
    <urn:Description>Determine date to send Continuers Enrolment Chase Letter</urn:Description>
    <urn:RuleStatement>
      <urn:Type>SQL_PREPARED_STATEMENT</urn:Type>
      <urn:Content>select NEXT_DAY(?,'MONDAY') + 21  as o_continuers_letter_date FROM DUAL</urn:Content>
    </urn:RuleStatement>
    <urn:RuleArgument>
      <urn:Name>o_sem_1_start_date</urn:Name>
      <urn:Description>Description for RuleArgument named input</urn:Description>
      <urn:Type>java.util.Date</urn:Type>
      <urn:InputFlag>true</urn:InputFlag>
      <urn:InputOrder>1</urn:InputOrder>
      <urn:InputRequired>true</urn:InputRequired>
    </urn:RuleArgument>
    <urn:RuleArgument>
      <urn:Name>o_continuers_letter_date</urn:Name>
      <urn:Description>Date when letter is sent to continuers</urn:Description>
      <urn:Type>java.util.Date</urn:Type>
      <urn:OutputFlag>true</urn:OutputFlag>
      <urn:OutputOrder>1</urn:OutputOrder>
      <urn:OutputGuaranteed>true</urn:OutputGuaranteed>
    </urn:RuleArgument>
    <urn:SystemReqInd>true</urn:SystemReqInd>
  </urn:Rule>
  <urn:Rule xmlns:urn="urn:sungardhe:relate:importhandlers:rules:1.0">
    <urn:Name>Get_Semester_1_Start_Date</urn:Name>
    <urn:Description>Determine start date of Semester 1</urn:Description>
    <urn:RuleStatement>
      <urn:Type>SQL_PREPARED_STATEMENT</urn:Type>
      <urn:Content>select sobptrm_start_date as o_sem_1_start_date    
from sobptrm    
where sobptrm_ptrm_code = 'S1'    and 
sobptrm_term_code =            
              (select stvterm_code FROM STVTERM             
       WHERE TRUNC (SYSDATE) &gt;= TRUNC (stvterm_start_date) AND 
    TRUNC (SYSDATE)   &lt;= TRUNC (stvterm_end_date)           
      )</urn:Content>
    </urn:RuleStatement>
    <urn:RuleArgument>
      <urn:Name>o_sem_1_start_date</urn:Name>
      <urn:Description>Start Date for Semester 1</urn:Description>
      <urn:Type>java.util.Date</urn:Type>
