  <urn:Rule xmlns:urn="urn:sungardhe:relate:importhandlers:rules:1.0">
    <urn:Name>Check_If_Run_Date_For_Continuers_Letter</urn:Name>
    <urn:Description>Check if today is the run date for continuers letter</urn:Description>
    <urn:RuleStatement>
      <urn:Type>SQL_PREPARED_STATEMENT</urn:Type>
      <urn:Content>select      (case when TRUNC(?) = TRUNC(SYSDATE)             
then 'Y'                         
else 'N'                
end) as v_run_today from dual</urn:Content>
    </urn:RuleStatement>
    <urn:RuleArgument>
      <urn:Name>o_continuers_letter_date</urn:Name>
      <urn:Description>Date when Continuers Letter should be sent</urn:Description>
      <urn:Type>java.util.Date</urn:Type>
