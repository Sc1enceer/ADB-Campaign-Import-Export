      <urn:LetterTemplate>
        <urn:PaperSettings>
          <urn:Dimension>in</urn:Dimension>
          <urn:TopMargin>0.5</urn:TopMargin>
          <urn:BottomMargin>0.2</urn:BottomMargin>
          <urn:LeftMargin>0.5</urn:LeftMargin>
