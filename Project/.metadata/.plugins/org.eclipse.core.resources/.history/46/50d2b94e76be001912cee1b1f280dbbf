  </urn:Population>
  <urn:Expression xmlns:urn="urn:sungardhe:relate:importhandlers:selection:1.1">
    <urn:Name>201920_Mark_Release_Presessional_Course_B</urn:Name>
    <urn:Description>PRJTASK0547732 - Marks release for pre-sessional students, 6th September 2019</urn:Description>
    <urn:Application>Relate</urn:Application>
    <urn:Creator>brmuser</urn:Creator>
    <urn:RawSql>true</urn:RawSql>
    <urn:ListType>student</urn:ListType>
    <urn:ExpressionDefinition>
      <urn:XmlExpression>&lt;?xml version="1.0" encoding="UTF-8" standalone="yes"?&gt;&lt;Expression xmlns="urn:com.sungardhe.common.services.selection.querygenerator"&gt;&lt;DatabaseColumnName&gt;SPRIDEN_PIDM&lt;/DatabaseColumnName&gt;&lt;DatabaseLocationName&gt;Student&lt;/DatabaseLocationName&gt;&lt;DataType&gt;number&lt;/DataType&gt;&lt;Subquery&gt;&lt;Statement&gt;SELECT spriden_pidm FROM sgbstdn,      spbpers,      goremal,      spriden,      sorlcur  a WHERE sgbstdn_term_code_eff = '201920'   AND sgbstdn_term_code_admit &amp;gt;= '201819'   AND  a.sorlcur_term_code = '201920' AND a.sorlcur_lmod_code = 'LEARNER'   AND a.sorlcur_cact_code = 'ACTIVE'   AND a.sorlcur_current_cde = 'Y'   AND a.sorlcur_program IN ('4291', '4292', '5083', '5084',  '6130', '8382', '8383', '8385', '8386', '8313')   AND sgbstdn_pidm = a.sorlcur_pidm   AND spbpers_pidm = a.sorlcur_pidm   AND spbpers_dead_ind IS NULL   AND goremal_pidm = a.sorlcur_pidm   AND  goremal_emal_code = 'UNI'   AND goremal_status_ind = 'A'   AND EXISTS (SELECT shrtckg_pidm               FROM shrtckg               WHERE shrtckg_pidm = a.sorlcur_pidm                 AND  shrtckg_term_code IN ('201819', '201920')                 AND shrtckg_gchg_code NOT IN ('IE', 'AU')                 AND TO_DATE (shrtckg_final_grde_chg_date) &amp;gt;= '01-AUG-2019') AND NOT EXISTS  (SELECT b.sorlcur_pidm                   FROM sorlcur b                   WHERE b.sorlcur_term_code &amp;lt; '201819'                     AND b.sorlcur_lmod_code = 'LEARNER'                     AND  b.sorlcur_pidm = a.sorlcur_pidm                     AND b.sorlcur_program IN (SELECT gtvsdax_external_code                                                FROM gtvsdax                                                WHERE gtvsdax_internal_code = 'PRSESS_CRS'                                                 AND gtvsdax_internal_code_group = 'PRSESS_CRS_CODE')) AND spriden_pidm = a.sorlcur_pidm AND  spriden_change_ind IS NULL&lt;/Statement&gt;&lt;Bindings&gt;&lt;Timestamp&gt;0&lt;/Timestamp&gt;&lt;Size&gt;0&lt;/Size&gt;&lt;/Bindings&gt;&lt;/Subquery&gt;&lt;/Expression&gt;</urn:XmlExpression>
    </urn:ExpressionDefinition>
  </urn:Expression>
