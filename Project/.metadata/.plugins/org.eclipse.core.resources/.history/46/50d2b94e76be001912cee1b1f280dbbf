    <urn:Name>201920_Mark_Release_Presessional_Course_B_Pop</urn:Name>
    <urn:Description>PRJTASK0547732 - Marks release for pre-sessional students, 6th September 2019</urn:Description>
    <urn:Owner>brmuser</urn:Owner>
    <urn:ListType>student</urn:ListType>
    <urn:ExecutionContext>
      <urn:ExpressionName>201920_Mark_Release_Presessional_Course_B</urn:ExpressionName>
