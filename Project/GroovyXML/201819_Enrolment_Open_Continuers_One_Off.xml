  <urn:Expression xmlns:urn="urn:sungardhe:relate:importhandlers:selection:1.1">
    <urn:Name>201819_Enrol_Open_Students_EXP</urn:Name>
    <urn:Description>Enrolment Open for Continuing Students</urn:Description>
    <urn:Application>Relate</urn:Application>
    <urn:Creator>brmuser</urn:Creator>
    <urn:RawSql>true</urn:RawSql>
    <urn:ListType>student</urn:ListType>
    <urn:ExpressionDefinition>
      <urn:XmlExpression>&lt;?xml version="1.0" encoding="UTF-8" standalone="yes"?&gt;&lt;Expression xmlns="urn:com.sungardhe.common.services.selection.querygenerator"&gt;&lt;DatabaseColumnName&gt;SPRIDEN_PIDM&lt;/DatabaseColumnName&gt;&lt;DatabaseLocationName&gt;Student&lt;/DatabaseLocationName&gt;&lt;DataType&gt;number&lt;/DataType&gt;&lt;Subquery&gt;&lt;Statement&gt;SELECT spriden_pidm   FROM spriden, sgbstdn, sfbetrm  WHERE     SGBSTDN.SGBSTDN_PIDM = SFBETRM.SFBETRM_PIDM        AND SGBSTDN.SGBSTDN_TERM_CODE_EFF = SFBETRM.SFBETRM_TERM_CODE        AND SGBSTDN.SGBSTDN_PIDM = SPRIDEN.SPRIDEN_PIDM        AND SGBSTDN.SGBSTDN_TERM_CODE_ADMIT &amp;lt; SGBSTDN.SGBSTDN_TERM_CODE_EFF        AND SPRIDEN.SPRIDEN_CHANGE_IND IS NULL        AND SFBETRM.SFBETRM_ESTS_CODE IN ('EL',                                          'EE',                                          'PE',                                          'RE',                                          'YE',                                          'PL',                                          'YL')        AND SGBSTDN.SGBSTDN_STST_CODE = 'AS'        AND SGBSTDN.SGBSTDN_CAMP_CODE NOT IN ('DL',                                              'EL',                                              'VT',                                              'SI',                                              'XX')        AND SGBSTDN.SGBSTDN_PROGRAM_1 NOT IN ('4326','5262')        AND SGBSTDN.SGBSTDN_PROGRAM_1 NOT IN (SELECT gtvsdax_external_code                                                FROM gtvsdax                                               WHERE     gtvsdax_internal_code_group =                                                            'PRSESS_CRS_CODE'                                                     AND gtvsdax_internal_code =                                                            'PRSESS_CRS')        AND SGBSTDN.SGBSTDN_TERM_CODE_EFF = :1        AND EXISTS               (SELECT 'true'                  FROM goremal                 WHERE     GOREMAL_PIDM = SPRIDEN.SPRIDEN_PIDM                       AND GOREMAL_STATUS_IND = 'A'                       AND GOREMAL_PREFERRED_IND = 'Y')        AND EXISTS               (SELECT 'true'                  FROM spbpers                 WHERE     SPBPERS.SPBPERS_PIDM = SPRIDEN.SPRIDEN_PIDM                       AND NVL (SPBPERS.SPBPERS_DEAD_IND, 'N') != 'Y')        AND BANINST1.FN$GET_GAINS_PREV_FYEAR (spriden_pidm, :1) &amp;lt;&amp;gt; 'Y'&lt;/Statement&gt;&lt;Bindings&gt;&lt;Timestamp&gt;0&lt;/Timestamp&gt;&lt;Size&gt;1&lt;/Size&gt;&lt;Binding&gt;&lt;Id&gt;1&lt;/Id&gt;&lt;BindingName&gt;i_TERM&lt;/BindingName&gt;&lt;Description&gt;Term&lt;/Description&gt;&lt;DataType&gt;string&lt;/DataType&gt;&lt;DataSource&gt;literal&lt;/DataSource&gt;&lt;Value&gt;201819&lt;/Value&gt;&lt;Property&gt;&lt;Name&gt;groupId&lt;/Name&gt;&lt;Value&gt;617E5B67-1D74-E2CE-2BC5-6B1C11F62E1A&lt;/Value&gt;&lt;/Property&gt;&lt;Property&gt;&lt;Name&gt;groupType&lt;/Name&gt;&lt;Value&gt;is&lt;/Value&gt;&lt;/Property&gt;&lt;/Binding&gt;&lt;/Bindings&gt;&lt;/Subquery&gt;&lt;/Expression&gt;</urn:XmlExpression>
    </urn:ExpressionDefinition>
  </urn:Expression>
