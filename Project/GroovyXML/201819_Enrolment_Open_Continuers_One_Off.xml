    <urn:CommunicationMethods>
      <urn:EmailTemplate>
        <urn:FromList>enrol@soton.ac.uk</urn:FromList>
        <urn:ToList>brmdev2@soton.ac.uk</urn:ToList>
        <urn:Subject>University of Southampton - Enrol</urn:Subject>
        <urn:Content>&lt;p class="MsoNormal" style="mso-margin-top-alt:auto;mso-margin-bottom-alt:auto"&gt;&lt;span style="mso-fareast-font-family:&amp;quot;Times New Roman&amp;quot;"&gt;&lt;br /&gt;&lt;/span&gt;&lt;/p&gt; &lt;table class="deviceWidth topheader" width="600" cellspacing="0" cellpadding="0" border="0" align="center"&gt;     &lt;tbody&gt;         &lt;tr&gt;             &lt;td valign="top" bgcolor="#002e3b" align="center"&gt;&lt;table width="601" cellspacing="0" cellpadding="0" border="0" align="center" height="112"&gt;                 &lt;tbody&gt;                     &lt;tr&gt;                         &lt;td class="em_side" width="45"&gt;&amp;nbsp;&lt;/td&gt;                         &lt;td valign="top" align="center"&gt;&lt;table width="100%" cellspacing="0" cellpadding="0" border="0" align="center"&gt;                             &lt;tbody&gt;                                 &lt;tr&gt;                                     &lt;td class="em_height" height="30"&gt;&amp;nbsp;&lt;/td&gt;                                 &lt;/tr&gt;                                 &lt;tr&gt;                                     &lt;td valign="top" align="left"&gt;&lt;table class="em_wrapper" cellspacing="0" cellpadding="0" border="0" align="left"&gt;                                         &lt;tbody&gt;                                             &lt;tr&gt;                                                 &lt;td valign="top" align="center"&gt;&lt;table cellspacing="0" cellpadding="0" border="0" align="center"&gt;                                                     &lt;tbody&gt;                                                         &lt;tr&gt;                                                             &lt;td valign="top" align="center"&gt;&lt;a href="http://www.southampton.ac.uk/" target="_blank" style="text-decoration:none;"&gt;&lt;img src="https://gallery.mailchimp.com/d14c4bc31e680281c5d4ef841/images/ea03ee46-e59b-41a4-ba77-7f14102f28fe.png" alt="UNIVERSITY OF Southampton" style="display:block; max-width:167px; font-family:Arial, sans-serif; font-size:22px; line-height:30px; color:#ffffff; font-weight:bold;" width="167" border="0" height="37" /&gt;&lt;/a&gt;&lt;/td&gt;                                                         &lt;/tr&gt;                                                     &lt;/tbody&gt;                                                 &lt;/table&gt;&lt;/td&gt;                                             &lt;/tr&gt;                                         &lt;/tbody&gt;                                     &lt;/table&gt;&lt;/td&gt;                                 &lt;/tr&gt;                                 &lt;tr&gt;                                     &lt;td class="em_height" height="30"&gt;&amp;nbsp;&lt;/td&gt;                                 &lt;/tr&gt;                             &lt;/tbody&gt;                         &lt;/table&gt;&lt;/td&gt;                         &lt;td class="em_side" width="45"&gt;&amp;nbsp;&lt;/td&gt;                     &lt;/tr&gt;                 &lt;/tbody&gt;             &lt;/table&gt;&lt;/td&gt;         &lt;/tr&gt;     &lt;/tbody&gt; &lt;/table&gt; &lt;p&gt;&amp;nbsp;&lt;/p&gt; &lt;table class="deviceWidth" width="600" cellspacing="0" cellpadding="0" border="0" bgcolor="#eeeeed" align="center"&gt;     &lt;tbody&gt;         &lt;tr&gt;             &lt;td style="padding: 5px 8px 10px; text-align: left; color: rgb(102, 102, 102); line-height: 24px; font-family: &amp;quot;Lucida Sans Unicode&amp;quot;, &amp;quot;Lucida Grande&amp;quot;, sans-serif; font-size: 15px; font-weight: normal; vertical-align: top;" bgcolor="#eeeeed"&gt;&lt;p style="text-align: center;"&gt;&lt;span style="font-family: Arial;"&gt;&lt;span style="font-size: 20pt;"&gt;Time to Enrol&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;             &lt;p style="line-height: 18pt; text-align: center;" class="MsoNormal"&gt;&lt;span style="font-family: Arial;"&gt;&lt;span style="color: rgb(102, 102, 102);"&gt;Hello |::=Constituent.Name Information.FirstName::|,&lt;/span&gt;&lt;/span&gt;&lt;span style="font-size: 12pt;"&gt;&lt;span style="color: rgb(102, 102, 102);"&gt;&lt;br /&gt;             &lt;/span&gt;&lt;/span&gt;&lt;span style="font-size: 11.5pt; color: rgb(102, 102, 102);"&gt;&lt;br /&gt;             &lt;/span&gt;&lt;span style="font-family: Arial;"&gt;Your Student ID number   |::=Constituent.Identifiers.Banner ID::| &lt;br /&gt;             Programme:   |::=Curricula.Student Program of Study.Primary Major Description(Term="201718")::|&lt;/span&gt;&lt;/p&gt;             &lt;p style="line-height: 18pt; text-align: center;" class="MsoNormal"&gt;&lt;span style="font-family: Arial;"&gt;It's  time to enrol for the 2018/19 academic year. Our academic year starts  on 1 August so even if you have enrolled recently you will need to do so  again.&lt;/span&gt;&lt;/p&gt;&lt;p style="line-height: 18pt; text-align: center;" class="MsoNormal"&gt;&amp;nbsp;&lt;/p&gt;&lt;table width="167" cellspacing="1" cellpadding="1" border="2" align="center" height="63"&gt;&lt;tbody&gt;&lt;tr&gt;&lt;td style="text-align: center;" bgcolor="#ff0000"&gt;&lt;a href="http://subscribe.soton.ac.uk/Checkepw"&gt;&lt;span style="font-family: Arial;"&gt;&lt;span style="font-size: 16pt;"&gt;&lt;span style="color: rgb(255, 255, 255);"&gt;Enrol now&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/a&gt;&lt;/td&gt;&lt;/tr&gt;&lt;/tbody&gt;&lt;/table&gt;&lt;p style="line-height: 18pt; text-align: center;" class="MsoNormal"&gt;&amp;nbsp;&lt;/p&gt;                                                    &lt;p style="text-align: center;"&gt;&lt;span style="font-size: 11.5pt; color: rgb(102, 102, 102);"&gt;             &lt;/span&gt;&lt;span style="color: rgb(102, 102, 102);"&gt;Please do not respond to this email but send any queries to &lt;/span&gt;&lt;span style="color: rgb(0, 124, 146);"&gt;&lt;a title="Email:enrol@soton.ac.uk" style="color: #007c92" href="mailto:enrol@soton.ac.uk?subject=Re%3A%20Time%20To%20Enrol&amp;amp;body=Note%20from%20Enrolment%3A%20Please%20provide%20your%20Student%20ID%20number%20on%20all%20emails.%0A"&gt;&lt;span style="color: rgb(0, 124, 146);"&gt;enrol@soton.ac.uk&lt;/span&gt;&lt;/a&gt;&lt;/span&gt;.&lt;/p&gt;&lt;/td&gt;         &lt;/tr&gt;     &lt;/tbody&gt; &lt;/table&gt; &lt;table class="deviceWidth" width="598" cellspacing="0" cellpadding="0" border="0" align="center" height="334"&gt;     &lt;tbody&gt;         &lt;tr&gt;             &lt;td style="padding-bottom: 5px; padding-top: 5px; padding-left: 0px; padding-right: 0px" bgcolor="#363636"&gt;&lt;br /&gt;&lt;table width="100%" cellspacing="0" cellpadding="0" border="0" align="center"&gt;                 &lt;tbody&gt;                     &lt;tr&gt;                         &lt;td class="em_side" width="50"&gt;&amp;nbsp;&lt;/td&gt;                         &lt;td valign="top" align="center"&gt;&lt;table width="488" cellspacing="0" cellpadding="0" border="0" align="center" height="262"&gt;                             &lt;tbody&gt;                                 &lt;tr&gt;                                     &lt;td class="em_height" height="40"&gt;&amp;nbsp;&lt;/td&gt;                                 &lt;/tr&gt;                                 &lt;tr&gt;                                     &lt;td class="em_blue" style="font-family:Arial, sans-serif; font-size:14px; line-height:24px; color:#62cae3;" align="center"&gt;&lt;a href="http://www.southampton.ac.uk/courses.page" target="_blank" style="text-decoration:underline; color:#62cae3;"&gt;Courses&lt;/a&gt; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;&lt;a href="http://www.southampton.ac.uk/uni-life.page" target="_blank" style="text-decoration:underline; color:#62cae3;"&gt;University life&lt;/a&gt; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;&lt;a href="http://www.southampton.ac.uk/research.page" target="_blank" style="text-decoration:underline; color:#62cae3;"&gt;Research&lt;/a&gt;&lt;span class="em_hide"&gt; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;&lt;/span&gt;&lt;a href="http://www.southa